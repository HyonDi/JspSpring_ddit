package com.bms.dto.notice;

import java.util.Date;

public class NoticeVO {
	
	private String notice_code;
	private String member_code;
	private String notice_title;
	private String notice_contents;
	private int notice_view_cnt;
	private Date notice_reg_date;
	private Date notice_update_date;
	private Date notice_start_date;
	private Date notice_last_date;
	private int notice_importance;
	private String board_sort_code;
	
	public String getNotice_code() {
		return notice_code;
	}
	public void setNotice_code(String notice_code) {
		this.notice_code = notice_code;
	}
	public String getMember_code() {
		return member_code;
	}
	public void setMember_code(String member_code) {
		this.member_code = member_code;
	}
	public String getNotice_title() {
		return notice_title;
	}
	public void setNotice_title(String notice_title) {
		this.notice_title = notice_title;
	}
	public String getNotice_contents() {
		return notice_contents;
	}
	public void setNotice_contents(String notice_contents) {
		this.notice_contents = notice_contents;
	}
	public int getNotice_view_cnt() {
		return notice_view_cnt;
	}
	public void setNotice_view_cnt(int notice_view_cnt) {
		this.notice_view_cnt = notice_view_cnt;
	}
	public Date getNotice_reg_date() {
		return notice_reg_date;
	}
	public void setNotice_reg_date(Date notice_reg_date) {
		this.notice_reg_date = notice_reg_date;
	}
	public Date getNotice_update_date() {
		return notice_update_date;
	}
	public void setNotice_update_date(Date notice_update_date) {
		this.notice_update_date = notice_update_date;
	}
	public Date getNotice_start_date() {
		return notice_start_date;
	}
	public void setNotice_start_date(Date notice_start_date) {
		this.notice_start_date = notice_start_date;
	}
	public Date getNotice_last_date() {
		return notice_last_date;
	}
	public void setNotice_last_date(Date notice_last_date) {
		this.notice_last_date = notice_last_date;
	}
	public int getNotice_importance() {
		return notice_importance;
	}
	public void setNotice_importance(int notice_importance) {
		this.notice_importance = notice_importance;
	}
	public String getBoard_sort_code() {
		return board_sort_code;
	}
	public void setBoard_sort_code(String board_sort_code) {
		this.board_sort_code = board_sort_code;
	}
	
	@Override
	public String toString() {
		return "NoticeVO [notice_code=" + notice_code + ", member_code=" + member_code + ", notice_title="
				+ notice_title + ", notice_contents=" + notice_contents + ", notice_view_cnt=" + notice_view_cnt
				+ ", notice_reg_date=" + notice_reg_date + ", notice_update_date=" + notice_update_date
				+ ", notice_start_date=" + notice_start_date + ", notice_last_date=" + notice_last_date
				+ ", notice_importance=" + notice_importance + ", board_sort_code=" + board_sort_code + "]";
	}

}
